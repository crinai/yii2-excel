    "keywords": [
        "yii2",
        "excel",
        "phpexcel",
        "import",
        "export",
        "active record"
    ],
    "homepage": "https://github.com/crinai/yii2-excel",
    "type": "yii2-extension",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "crinai",
            "email": "crinai@crinai.com"
        }
    ],
    "require": {
        "yiisoft/yii2" : "*",
		"phpoffice/phpspreadsheet" : "*"
    },
    "require-dev": {
        "yiisoft/yii2-codeception": "~2.0"
    },
    
    "autoload": {
        "psr-4": {
            "crinai\\excel\\": "src"
        }
    }
